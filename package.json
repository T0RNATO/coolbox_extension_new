{
  "name": "coolbox",
  "type": "module",
  "version": "1.2.3",
  "displayName": "Coolbox",
  "author": "TorNato & SleepyStew",
  "description": "An extension to extend and customise Schoolbox.",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://schoolbox.donvale.vic.edu.au\" --source-dir ./dist/"
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^5.1.0",
    "@types/chrome": "^0.0.228",
    "@types/webextension-polyfill": "^0.10.7",
    "@vitejs/plugin-vue": "^4.6.2",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.10.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "vite": "6.0.9",
    "web-ext": "^7.11.0"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^5.4.0",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "dompurify": "3.2.4",
    "uuid": "^9.0.1",
    "vue": "^3.4.25",
    "vue-shadow-dom": "^4.2.0",
    "vue3-smooth-dnd": "^0.0.5",
    "webextension-polyfill": "^0.10.0"
  }
}
