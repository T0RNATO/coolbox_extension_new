{
  "name": "coolbox",
  "type": "module",
  "version": "1.2.8",
  "displayName": "Coolbox",
  "author": "TorNato & SleepyStew",
  "description": "An extension to extend and customise Schoolbox.",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://schoolbox.donvale.vic.edu.au\" --source-dir ./dist/"
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^5.1.1",
    "@types/chrome": "^0.0.322",
    "@types/webextension-polyfill": "^0.12.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.10.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3",
    "vite": "^5",
    "web-ext": "^8.6.0"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^11.0.2",
    "@vueuse/components": "^13.1.0",
    "@vueuse/core": "^13.1.0",
    "caniuse-lite": "^1.0.30001717",
    "dompurify": "^3.2.5",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-shadow-dom": "^4.2.0",
    "vue3-smooth-dnd": "^0.0.6",
    "webextension-polyfill": "^0.12.0"
  }
}
